<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>SecureBankingSystem</display-name>
  <welcome-file-list>
    <welcome-file>/webpages/credit.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>DebitAndCreditServlet</servlet-name>
    <servlet-class>com.asu.ss.secure_banking_system.servlet.DebitAndCreditServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DebitAndCreditServlet</servlet-name>
    <url-pattern>/credit.jsp</url-pattern>
  </servlet-mapping>
</web-app>